{
  "manifest_version": 2,
  "name": "Looi",
  "version": "1.1.0",
  "description": "A Minimal, Customisable New Tab page for Firefox, Chrome, and Edge.",
  "browser_specific_settings": {
  "gecko": {
    "id": "7e332fc3-46f0-4753-b8a1-e99f91470790@looi-extension"
  }
},
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "https://github.com/",
    "https://api.github.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "icons/looi-16.png",
    "48": "icons/looi-48.png",
    "96": "icons/looi-96.png",
    "128": "icons/looi-128.png"
  }
}
